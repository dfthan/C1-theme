{
	"name": "C1 Theme",
	"colors": {
		"activityBar.background": "#151515",
		"editor.background": "#121212",
		"editor.foreground": "#cecece",
		"editor.lineHighlightBackground": "#232d38",
		"editor.selectionBackground": "#354454",
		"editorCursor.background": "#0d0208",
		"editorCursor.foreground": "#008f11",
		"editorGroupHeader.tabsBackground": "#121212",
		"editorWhitespace.foreground": "#303f4f",
		"editorWidget.background": "#121212",
		"sideBar.background": "#141414",
		"sideBarSectionHeader.background": "#121212",
		"statusBar.background": "#141414",
		"statusBarItem.remoteBackground": "#232323",
		"tab.inactiveBackground": "#0f0f0f",
		"titleBar.activeBackground": "#131313"
	},
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#9e9e9e",
				"fontStyle": " italic"
			}
		},
		{
			"scope": "meta.parameters comment.block",
			"settings": {
				"foreground": "#42675A",
				"fontStyle": " italic"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#99aadd"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#99aadd"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#99aadd"
			}
		},
		{
			"scope": "constant.character, constant.other, variable.other.constant",
			"settings": {
				"foreground": "#99aadd",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "variable, support.other.variable",
			"settings": {
				"foreground": "#aaaaff",
				"fontStyle": ""
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#995599"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#995599",
				"fontStyle": ""
			}
